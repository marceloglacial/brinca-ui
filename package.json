{
    "name": "@marceloglacial/brinca-ui",
    "version": "0.12.0",
    "author": "Marcelo Glacial",
    "types": "dist/index.d.ts",
    "module": "dist/brinca-ui.esm.js",
    "files": [
        "dist"
    ],
    "exports": {
        ".": {
            "import": "./dist/brinca-ui.es.js",
            "require": "./dist/brinca-ui.esm.js"
        }
    },
    "scripts": {
        "dev": "yarn storybook",
        "build": "rm -rf dist && yarn lint && yarn format && tsc && vite build",
        "build:docs": "build-storybook -o docs",
        "serve": "vite preview",
        "storybook": "start-storybook -p 6006 --modern",
        "format": "prettier -w src",
        "lint": "eslint --fix src/**/*.ts*",
        "start": "start-storybook -p 6006 --ci --quiet",
        "test": "concurrently \"yarn dev\" \"yarn run cypress open\"",
        "test:ci": "concurrently \"yarn start\" \"cypress run --headless\"",
        "prerelease": "yarn build && yarn build:docs && yarn test:ci"
    },
    "dependencies": {
        "concurrently": "^7.4.0",
        "prettier": "^2.7.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "styled-components": "^5.3.5"
    },
    "devDependencies": {
        "@babel/core": "^7.19.1",
        "@mdx-js/react": "^2.1.3",
        "@rollup/plugin-typescript": "^8.5.0",
        "@storybook/addon-actions": "^6.5.12",
        "@storybook/addon-docs": "^6.5.12",
        "@storybook/addon-essentials": "^6.5.12",
        "@storybook/addon-links": "^6.5.12",
        "@storybook/builder-vite": "^0.2.2",
        "@storybook/react": "^6.5.12",
        "@tailwindcss/jit": "^0.1.18",
        "@types/react": "^18.0.21",
        "@types/react-dom": "^18.0.6",
        "@types/styled-components": "^5.1.26",
        "@typescript-eslint/eslint-plugin": "^5.38.0",
        "@typescript-eslint/parser": "^5.38.0",
        "@vitejs/plugin-react": "^2.1.0",
        "autoprefixer": "^10.4.12",
        "babel-loader": "^8.2.5",
        "cypress": "^10.8.0",
        "eslint": "^8.24.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-storybook": "^0.6.4",
        "postcss": "^8.4.16",
        "tailwindcss": "^3.1.8",
        "typescript": "^4.8.3",
        "vite": "^3.1.3"
    }
}
